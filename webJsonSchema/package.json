{
  "name": "graph-json",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run generate-schema && vite",
    "build": "npm run generate-schema && run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "generate-schema": "ts-json-schema-generator --path types/models.ts --type Main --out types/schema.json"
  },
